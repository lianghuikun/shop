.tabs .tabs_title {
  /* 伸缩盒子 */
  display: flex;
}
.tabs .tabs_title .title_item {
  display: flex;
  justify-content: center;
  align-items: center;
  flex: 1;
  /* 上下15rpx，左右不给 */
  padding: 15rpx 0;
}
.tabs .tabs_title .active {
  color: var(--themeColor);
  border-bottom: 5px solid currentColor;
}
